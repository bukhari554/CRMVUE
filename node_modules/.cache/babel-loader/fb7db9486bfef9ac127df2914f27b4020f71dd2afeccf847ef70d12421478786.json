 return;\n  }\n\n  loading.value = true;\n  message.value = \"\";\n\n  try {\n    // Call API to register\n    const response = await fetch(\"http://localhost:8000/api/register\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n        \"Accept\": \"application/json\",\n        \"Authorization\": \"Bearer 1|CdfTfQNwEi1RAlFnueMNhH11RgMM9B8LCF78OvXw6c411fce\"\n      },\n      body: JSON.stringify({\n        account_type: formData.value.account_type,\n        first_name: formData.value.first_name,\n        last_name: formData.value.last_name,\n        email: formData.value.email,\n        password: formData.value.password,\n        password_confirmation: formData.value.password_confirmation,\n        country: formData.value.country,\n        country_code: formData.value.country_code,\n        telephone: formData.value.telephone,\n        currency: formData.value.currency\n      })\n    });\n\n    const data = await response.json();\n\n    if (response.ok) {\n      message.value = \"Sign up successful! You can now login.\";\n      messageType.value = \"success\";\n      \n      // Clear form data\n      formData.value = {\n        account_type: \"Select Account Type\",\n        first_name: \"\",\n        last_name: \"\",\n        email: \"\",\n        password: \"\",\n        password_confirmation: \"\",\n        currency: \"USD\",\n        country: \"\",\n        country_code: \"\",\n        telephone: \"\",\n        agree: false\n      };\n      countryCodeInput.value = \"\";\n\n      // Redirect to login page after 2 seconds\n      setTimeout(() => {\n        window.location.href = \"/login\";\n      }, 2000);\n    } else {\n      message.value = data.message || \"Something went wrong\";\n      messageType.value = \"error\";\n    }\n  } catch (error) {\n    console.error(\"Error:\", error);\n    message.value = \"Could not connect to server\";\n    messageType.value = \"error\";\n  } finally {\n    loading.value = false;