{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport \"core-js/modules/esnext.set.difference.v2.js\";\nimport \"core-js/modules/esnext.set.intersection.v2.js\";\nimport \"core-js/modules/esnext.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/esnext.set.is-subset-of.v2.js\";\nimport \"core-js/modules/esnext.set.is-superset-of.v2.js\";\nimport \"core-js/modules/esnext.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/esnext.set.union.v2.js\";\nimport { onBeforeUnmount, onBeforeMount, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { countries } from \"@/Data/countries.js\";\nimport Navbar from \"@/examples/PageLayout/Navbarsignup.vue\";\nimport AppFooter from \"@/examples/PageLayout/Footer.vue\";\nimport ArgonInput from \"@/components/ArgonInput.vue\";\nimport ArgonCheckbox from \"@/components/ArgonCheckbox.vue\";\nimport ArgonButton from \"@/components/ArgonButton.vue\";\nexport default {\n  __name: 'Signup',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const body = document.getElementsByTagName(\"body\")[0];\n    const store = useStore();\n    const accountTypes = ref([\"Individual\", \"Company\"]);\n    const currencies = ref([\"USD\"]);\n\n    // Form data object\n    const formData = ref({\n      account_type: \"Select Account Type\",\n      first_name: \"\",\n      last_name: \"\",\n      email: \"\",\n      password: \"\",\n      password_confirmation: \"\",\n      currency: \"USD\",\n      country: \"\",\n      country_code: \"\",\n      telephone: \"\",\n      agree: false\n    });\n    const loading = ref(false);\n    const message = ref(\"\");\n    const messageType = ref(\"\");\n    const telephoneError = ref(\"\");\n    const countryCodeInput = ref(\"\");\n\n    // Get unique country codes for dropdown\n    const uniqueCountryCodes = ref([...new Set(countries.map(c => c.code))].sort());\n\n    // Get country by code\n    const getCountryByCode = code => {\n      return countries.find(c => c.code === code);\n    };\n\n    // When country is selected, update country code\n    const onCountryChange = () => {\n      const selected = countries.find(c => c.name === formData.value.country);\n      if (selected) {\n        formData.value.country_code = selected.code;\n        countryCodeInput.value = selected.code;\n      }\n    };\n\n    // When country code is selected/changed, update country name\n    const onCountryCodeChange = code => {\n      formData.value.country_code = code;\n      countryCodeInput.value = code;\n\n      // Find first country with this code\n      const country = getCountryByCode(code);\n      if (country) {\n        formData.value.country = country.name;\n      }\n    };\n\n    // Validate phone number based on country\n    const validatePhoneNumber = () => {\n      telephoneError.value = \"\";\n      const phone = formData.value.telephone.trim();\n      const countryCode = formData.value.country_code;\n      if (!phone) {\n        return true;\n      }\n\n      // Remove spaces and hyphens\n      const cleanPhone = phone.replace(/[\\s-]/g, \"\");\n\n      // Check if phone starts with country code or +\n      if (!cleanPhone.startsWith(\"+\") && !cleanPhone.startsWith(countryCode.replace(\"+\", \"\"))) {\n        telephoneError.value = `Phone number should start with ${countryCode} or ${countryCode.replace(\"+\", \"\")}`;\n        return false;\n      }\n\n      // Check if phone contains only digits and allowed characters\n      if (!/^[\\d\\s+()]*$/.test(phone)) {\n        telephoneError.value = \"Phone number contains invalid characters\";\n        return false;\n      }\n\n      // Check minimum length (usually 9-15 digits)\n      const digitsOnly = cleanPhone.replace(/\\D/g, \"\");\n      if (digitsOnly.length < 9 || digitsOnly.length > 15) {\n        telephoneError.value = \"Phone number should be between 9-15 digits\";\n        return false;\n      }\n      return true;\n    };\n    onBeforeMount(() => {\n      store.state.hideConfigButton = true;\n      store.state.showNavbar = false;\n      store.state.showSidenav = false;\n      store.state.showFooter = false;\n      body.classList.remove(\"bg-gray-100\");\n    });\n    onBeforeUnmount(() => {\n      store.state.hideConfigButton = false;\n      store.state.showNavbar = true;\n      store.state.showSidenav = true;\n      store.state.showFooter = true;\n      body.classList.add(\"bg-gray-100\");\n    });\n\n    // Submit signup form function\n    const submitsignup = async () => {\n      // Validation check\n      if (!formData.value.account_type || !formData.value.first_name || !formData.value.last_name || !formData.value.email || !formData.value.password) {\n        message.value = \"Please fill all required fields\";\n        messageType.value = \"error\";\n        return;\n      }\n      if (formData.value.password !== formData.value.password_confirmation) {\n        message.value = \"Passwords do not match\";\n        messageType.value = \"error\";\n        return;\n      }\n      if (!formData.value.agree) {\n        message.value = \"Please agree to Terms and Conditions\";\n        messageType.value = \"error\";\n        return;\n      }\n      if (formData.value.telephone && !validatePhoneNumber()) {\n        message.value = telephoneError.value;\n        messageType.value = \"error\";\n        return;\n      }\n      loading.value = true;\n      message.value = \"\";\n      try {\n        // Call API to register\n        const response = await fetch(\"http://localhost:8000/api/register\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            \"Accept\": \"application/json\",\n            \"Authorization\": \"Bearer 1|CdfTfQNwEi1RAlFnueMNhH11RgMM9B8LCF78OvXw6c411fce\"\n          },\n          body: JSON.stringify({\n            account_type: formData.value.account_type,\n            first_name: formData.value.first_name,\n            last_name: formData.value.last_name,\n            email: formData.value.email,\n            password: formData.value.password,\n            password_confirmation: formData.value.password_confirmation,\n            country: formData.value.country,\n            country_code: formData.value.country_code,\n            telephone: formData.value.telephone,\n            currency: formData.value.currency\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          message.value = \"Sign up successful! You can now login.\";\n          messageType.value = \"success\";\n\n          // Clear form data\n          formData.value = {\n            account_type: \"Select Account Type\",\n            first_name: \"\",\n            last_name: \"\",\n            email: \"\",\n            password: \"\",\n            password_confirmation: \"\",\n            currency: \"USD\",\n            country: \"\",\n            country_code: \"\",\n            telephone: \"\",\n            agree: false\n          };\n          countryCodeInput.value = \"\";\n\n          // Redirect to login page after 2 seconds\n          setTimeout(() => {\n            window.location.href = \"/login\";\n          }, 2000);\n        } else {\n          message.value = data.message || \"Something went wrong\";\n          messageType.value = \"error\";\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n        message.value = \"Could not connect to server\";\n        messageType.value = \"error\";\n      } finally {\n        loading.value = false;\n      }\n    };\n    const __returned__ = {\n      body,\n      store,\n      accountTypes,\n      currencies,\n      formData,\n      loading,\n      message,\n      messageType,\n      telephoneError,\n      countryCodeInput,\n      uniqueCountryCodes,\n      getCountryByCode,\n      onCountryChange,\n      onCountryCodeChange,\n      validatePhoneNumber,\n      submitsignup,\n      onBeforeUnmount,\n      onBeforeMount,\n      ref,\n      get useStore() {\n        return useStore;\n      },\n      get countries() {\n        return countries;\n      },\n      Navbar,\n      AppFooter,\n      ArgonInput,\n      ArgonCheckbox,\n      ArgonButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onBeforeUnmount","onBeforeMount","ref","useStore","countries","Navbar","AppFooter","ArgonInput","ArgonCheckbox","ArgonButton","body","document","getElementsByTagName","store","accountTypes","currencies","formData","account_type","first_name","last_name","email","password","password_confirmation","currency","country","country_code","telephone","agree","loading","message","messageType","telephoneError","countryCodeInput","uniqueCountryCodes","Set","map","c","code","sort","getCountryByCode","find","onCountryChange","selected","name","value","onCountryCodeChange","validatePhoneNumber","phone","trim","countryCode","cleanPhone","replace","startsWith","test","digitsOnly","length","state","hideConfigButton","showNavbar","showSidenav","showFooter","classList","remove","add","submitsignup","response","fetch","method","headers","JSON","stringify","data","json","ok","setTimeout","window","location","href","error","console"],"sources":["/home/aurora/Videos/Forex_CRM_Vue/src/views/Signup.vue"],"sourcesContent":["<script setup>\nimport { onBeforeUnmount, onBeforeMount, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { countries } from \"@/Data/countries.js\";\nimport Navbar from \"@/examples/PageLayout/Navbarsignup.vue\";\nimport AppFooter from \"@/examples/PageLayout/Footer.vue\";\nimport ArgonInput from \"@/components/ArgonInput.vue\";\nimport ArgonCheckbox from \"@/compon