pe</label>\n                  <select v-model=\"formData.account_type\" class=\"form-control\" required>\n                    <option value=\"\">Select Account Type</option>\n                    <option v-for=\"type in accountTypes\" :key=\"type\" :value=\"type\">{{ type }}</option>\n                  </select>\n                </div>\n\n                <!-- First and Last Name -->\n                <div class=\"row\">\n                  <div class=\"col-md-6\">\n                    <argon-input v-model=\"formData.first_name\" placeholder=\"First Name\" />\n                  </div>\n                  <div class=\"col-md-6\">\n                    <argon-input v-model=\"formData.last_name\" placeholder=\"Last Name\" />\n                  </div>\n                </div>\n\n                <!-- Email -->\n                <argon-input v-model=\"formData.email\" type=\"email\" placeholder=\"Email\" />\n\n                <!-- Country -->\n                <div class=\"mb-3\">\n                  <label class=\"form-label\">Country</label>\n                  <select v-model=\"formData.country\" @change=\"onCountryChange\" class=\"form-control\">\n                    <option value=\"\">Select Country</option>\n                    <option v-for=\"country in countries\" :key=\"country.name\" :value=\"country.name\">\n                      {{ country.name }}\n                    </option>\n                  </select>\n                </div>\n\n                <!-- Telephone -->\n                <div class=\"row\">\n                  <div class=\"col-md-5\">\n                    <label class=\"form-label\">Country Code</label>\n                    <select :value=\"formData.country_code\" @change=\"onCountryCodeChange($event.target.value)\" class=\"form-control\">\n                      <option value=\"\">Select Code</option>\n                      <option v-for=\"code in uniqueCountryCodes\" :key=\"code\" :value=\"code\">{{ code }}</option>\n                    </select>\n          