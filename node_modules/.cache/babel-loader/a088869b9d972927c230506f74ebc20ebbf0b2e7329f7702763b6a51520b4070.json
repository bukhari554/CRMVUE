 => c.code === code);\n};\n\n// When country is selected, update country code\nconst onCountryChange = () => {\n  const selected = countries.find(c => c.name === formData.value.country);\n  if (selected) {\n    formData.value.country_code = selected.code;\n  }\n};\n\n// When country code is selected/changed, update country name\nconst onCountryCodeChange = (code) => {\n  formData.value.country_code = code;\n  const country = getCountryByCode(code);\n  if (country) {\n    formData.value.country = country.name;\n  }\n};\n\n// Validate phone number based on country\nconst validatePhoneNumber = () => {\n  telephoneError.value = \"\";\n  const phone = formData.value.telephone.trim();\n  const countryCode = formData.value.country_code;\n\n  if (!phone) return true;\n\n  const cleanPhone = phone.replace(/[\\s-]/g, \"\");\n  if (!cleanPhone.startsWith(\"+\") && !cleanPhone.startsWith(countryCode.replace(\"+\", \"\"))) {\n    telephoneError.value = `Phone number should start with ${countryCode}`;\n    return false;\n  }\n  if (!/^[\\d\\s+()]*$/.test(phone)) {\n    telephoneError.value = \"Phone number contains invalid characters\";\n    return false;\n  }\n  const digitsOnly = cleanPhone.replace(/\\D/g, \"\");\n  if (digitsOnly.length < 9 || digitsOnly.length > 15) {\n    telephoneError.value = \"Phone number should be between 9-15 digits\";\n    return false;\n  }\n  return true;\n};\n\nonBeforeMount(() => {\n  store.state.hideConfigButton = true;\n  store.state.showNavbar = false;\n  store.state.showSidenav = false;\n  store.state.showFooter = false;\n  body.classList.remove(\"bg-gray-100\");\n});\n\nonBeforeUnmount(() => {\n  store.state.hideConfigButton = false;\n  store.state.showNavbar = true;\n  store.state.showSidenav = true;\n  store.state.showFooter = true;\n  body.classList.add(\"bg-gray-100\");\n});\n\n// Submit signup form function\nconst submitsignup = async () => {\n  if (!formData.value.account_type || !formData.value.first_name || !formData.value.last_name |