ntryByCode = code => {\n      return countries.find(c => c.code === code);\n    };\n\n    // When country is selected, update country code\n    const onCountryChange = () => {\n      const selected = countries.find(c => c.name === formData.value.country);\n      if (selected) {\n        formData.value.country_code = selected.code;\n      }\n    };\n\n    // When country code is selected/changed, update country name\n    const onCountryCodeChange = code => {\n      formData.value.country_code = code;\n      const country = getCountryByCode(code);\n      if (country) {\n        formData.value.country = country.name;\n      }\n    };\n\n    // Validate phone number based on country\n    const validatePhoneNumber = () => {\n      telephoneError.value = \"\";\n      const phone = formData.value.telephone.trim();\n      const countryCode = formData.value.country_code;\n      if (!phone) return true;\n      const cleanPhone = phone.replace(/[\\s-]/g, \"\");\n      if (!cleanPhone.startsWith(\"+\") && !cleanPhone.startsWith(countryCode.replace(\"+\", \"\"))) {\n        telephoneError.value = `Phone number should start with ${countryCode}`;\n        return false;\n      }\n      if (!/^[\\d\\s+()]*$/.test(phone)) {\n        telephoneError.value = \"Phone number contains invalid characters\";\n        return false;\n      }\n      const digitsOnly = cleanPhone.replace(/\\D/g, \"\");\n      if (digitsOnly.length < 9 || digitsOnly.length > 15) {\n        telephoneError.value = \"Phone number should be between 9-15 digits\";\n        return false;\n      }\n      return true;\n    };\n    onBeforeMount(() => {\n      store.state.hideConfigButton = true;\n      store.state.showNavbar = false;\n      store.state.showSidenav = false;\n      store.state.showFooter = false;\n      body.classList.remove(\"bg-gray-100\");\n    });\n    onBeforeUnmount(() => {\n      store.state.hideConfigButton = false;\n      store.state.showNavbar = true;\n      store.state.showSidenav = true;\n      store.state.showFooter = true;\n      body.classList.add(\"bg-gray-100\");\n    });\n\n    // Submit signup form function\n    const submitsignup = async () => {\n      if (!formData.value.account_type || !formData.value.first_name || !formData.value.last_name || !formData.value.email || !formData.value.password) {\n        message.value = \"Please fill all required fields\";\n        messageType.value = \"error\";\n        return;\n      }\n      if (formData.value.password !== formData.value.password_confirmation) {\n        message.value = \"Passwords do not match\";\n        messageType.value = \"error\";\n        return;\n      }\n      if (!formData.value.agree) {\n        message.value = \"Please agree to Terms and Conditions\";\n        messageType.value = \"error\";\n        return;\n      }\n      if (formData.value.telephone && !validatePhoneNumber()) {\n        message.value = telephoneError.value;\n        messageType.value = \"error\";\n        return;\n      }\n      loading.value = true;\n      message.value = \"\";\n      try {\n        const response = await fetch(\"http://localhost:8000/api/register\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            \"Accept\": \"application/json\",\n            \"Authorization\": \"Bearer 1|CdfTfQNwEi1RAlFnueMNhH11RgMM9B8LCF78OvXw6c411fce\"\n          },\n          body: JSON.stringify({\n            account_type: formData.value.account_type,\n            first_name: formData.value.first_name,\n            last_name: formData.value.last_name,\n            email: formData.value.email,\n            password: formData.value.password,\n            password_confirmation: formData.value.password_confirmation,\n            country: formData.value.country,\n            country_code: formData.value.country_code,\n            telephone: formData.value.telephone,\n            currency: formData.value.currency\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          message.value = \"Sign up successful! You can now login.\";\n          messageType.value = \"success\";\n          formData.value = {\n            account_type: \"\",\n            first_name: \"\",\n            last_name: \"\",\n            email: \"\",\n            password: \"\",\n            password_confirmation: \"\",\n            currency: \"\",\n            country: \"\",\n            country_code: \"\",\n            telephone: \"\",\n            agree: false\n          };\n          setTimeout(() => {\n            window.location.href = \"/login\";\n          }, 2000);\n        } else {\n          message.value = data.message || \"Something went wrong\";\n          messageType.value = \"error\";\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n        message.value = \"Could not connect to server\";\n        messageType.value = \"error\";\n      } finally {\n        loading.value = false;\n      }\n    };\n    const __returned__ = {\n      body,\n      store,\n      accountTypes,\n      currencies,\n      formData,\n      loading,\n      message,\n      messageType,\n      telephoneError,\n      uniqueCountryCodes,\n      getCountryByCode,\n      onCountryChange,\n      onCountryCodeChange,\n      validatePhoneNumber,\n      submitsignup,\n      onBeforeUnmount,\n      onBeforeMount,\n      ref,\n      get useStore() {\n        return useStore;\n      },\n      get countries() {\n        return countries;\n      },\n      Navbar,\n      AppFooter,\n      ArgonInput,\n      ArgonButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onBeforeUnmount","onBeforeMount","ref","useStore","countries","Navbar","AppFooter","ArgonInput","ArgonButton","body","document","getElementsByTagName","store","accountTypes","currencies","formDat