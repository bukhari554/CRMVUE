{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { activateDarkMode, deactivateDarkMode } from \"@/assets/js/dark-mode\";\nexport default {\n  __name: 'Configurator',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    // state\n    const isNavFixed = computed(() => store.state.isNavFixed);\n    const sidebarType = computed(() => store.state.sidebarType);\n    const toggleConfigurator = () => store.commit(\"toggleConfigurator\");\n\n    // mutations\n    const navbarFixed = () => store.commit(\"navbarFixed\");\n    const setSidebarType = type => store.commit(\"sidebarType\", type);\n    const sidebarColor = (color = \"success\") => {\n      document.querySelector(\"#sidenav-main\").setAttribute(\"data-color\", color);\n    };\n    const darkMode = () => {\n      if (store.state.darkMode) {\n        store.state.darkMode = false;\n        setSidebarType(\"bg-white\");\n        deactivateDarkMode();\n        return;\n      } else {\n        store.state.darkMode = true;\n        setSidebarType(\"bg-default\");\n        activateDarkMode();\n      }\n    };\n    const __returned__ = {\n      store,\n      isNavFixed,\n      sidebarType,\n      toggleConfigurator,\n      navbarFixed,\n      setSidebarType,\n      sidebarColor,\n      darkMode,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      get activateDarkMode() {\n        return activateDarkMode;\n      },\n      get deactivateDarkMode() {\n        return deactivateDarkMode;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useStore","activateDarkMode","deactivateDarkMode","store","isNavFixed","state","sidebarType","toggleConfigurator","commit","navbarFixed","setSidebarType","type","sidebarColor","color","document","querySelector","setAttribute","darkMode"],"sources":["/home/aurora/Videos/Forex_CRM_Vue/src/examples/Configurator.vue"],"sourcesContent":["<script setup>\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { activateDarkMode, deactivateDarkMode } from \"@/assets/js/dark-mode\";\n\nconst store = useStore();\n// state\nconst isNavFixed = computed(() => store.state.isNavFixed);\nconst sidebarType = computed(() => store.state.sidebarType);\nconst toggleConfigurator = () => store.commit(\"toggleConfigurator\");\n\n// mutations\nconst navbarFixed = () => store.commit(\"navbarFixed\");\nconst setSidebarType = (type) => store.commit(\"sidebarType\", type);\n\nconst sidebarColor = (color = \"success\") => {\n  document.querySelector(\"#sidenav-main\").setAttribute(\"data-color\", color);\n};\n\nconst darkMode = () => {\n  if (store.state.darkMode) {\n    store.state.darkMode = false;\n    setSidebarType(\"bg-white\");\n    deactivateDarkMode();\n    return;\n  } else {\n    store.state.darkMode = true;\n    setSidebarType(\"bg-default\");\n    activateDarkMode();\n  }\n};\n</script>\n<template>\n  <div class=\"fixed-plugin\">\n    <a\n      class=\"px-3 py-2 fixed-plugin-button text-dark position-fixed\"\n      @click=\"toggleConfigurator\"\n    >\n      <i class=\"py-2 fa fa-cog\"></i>\n    </a>\n    <div class=\"shadow-lg card\">\n      <div class=\"pt-3 pb-0 bg-transparent card-header\">\n        <div class=\"float-start\">\n          <h5 class=\"mt-3 mb-0\">Argon Configurator</h5>\n          <p>See our dashboard options.</p>\n        </div>\n        <div\n          class=\"mt-4 float-end\"\n          @click=\"toggleConfigurator\"\n        >\n          <button class=\"p-0 btn btn-link text-dark fixed-plugin-close-button\">\n            <i class=\"fa fa-close\"></i>\n          </button>\n        </div>\n        <!-- End Toggle Button -->\n      </div>\n      <hr class=\"my-1 horizontal dark\" />\n      <div class=\"pt-0 card-body pt-sm-3\">\n        <!-- Sidebar Backgrounds -->\n        <div>\n          <h6 class=\"mb-0\">Sidebar Colors</h6>\n        </div>\n        <a href=\"#\" class=\"switch-trigger background-color\">\n          <div\n            class=\"my-2 badge-colors text-start\"\n          >\n            <span\n              class=\"badge filter bg-gradient-primary active\"\n              data-color=\"primary\"\n              @click=\"sidebarColor('primary')\"\n            ></span>\n            <span\n              class=\"badge filter bg-gradient-dark\"\n              data-color=\"dark\"\n              @click=\"sidebarColor('dark')\"\n            ></span>\n            <span\n              class=\"badge filter bg-gradient-info\"\n              data-color=\"info\"\n              @click=\"sidebarColor('info')\"\n            ></span>\n            <span\n              class=\"badge filter bg-gradient-success\"\n              data-color=\"success\"\n              @click=\"sidebarColor('success')\"\n            ></span>\n            <span\n              class=\"badge filter bg-gradient-warning\"\n              data-color=\"warning\"\n              @click=\"sidebarColor('warning')\"\n            ></span>\n            <span\n              class=\"badge filter bg-gradient-danger\"\n              data-color=\"danger\"\n              @click=\"sidebarColor('danger')\"\n            ></span>\n          </div>\n        </a>\n        <!-- Sidenav Type -->\n        <div class=\"mt-3\">\n          <h6 class=\"mb-0\">Sidenav Type</h6>\n          <p class=\"text-sm\">Choose between 2 different sidenav types.</p>\n        </div>\n        <div class=\"d-flex gap-2\">\n          <button\n            id=\"btn-white\"\n            class=\"btn w-100 px-3 mb-2\"\n            :class=\"\n              sidebarType === 'bg-white'\n                ? 'bg-gradient-success'\n                : 'btn-outline-success'\n            \"\n            @click=\"setSidebarType('bg-white')\"\n          >\n            White\n          </button>\n          <button\n            id=\"btn-dark\"\n            class=\"btn w-100 px-3 mb-2\"\n            :class=\"\n              sidebarType === 'bg-default'\n                ? 'bg-gradient-success'\n                : 'btn-outline-success'\n            \"\n            @click=\"setSidebarType('bg-default')\"\n          >\n            Dark\n          </button>\n        </div>\n        <p class=\"mt-2 text-sm d-xl-none d-block\">\n          You can change the sidenav type just on desktop view.\n        </p>\n        <!-- Navbar Fixed -->\n        <!-- Navbar Fixed -->\n        <div class=\"mt-3 d-flex\">\n          <h6 class=\"mb-0\">Navbar Fixed</h6>\n          <div class=\"form-check form-switch ps-0 ms-auto my-auto\">\n            <input\n              class=\"mt-1 form-check-input ms-auto\"\n              type=\"checkbox\"\n              id=\"navbarFixed\"\n              :checked=\"isNavFixed\"\n              @click=\"navbarFixed\"\n            />\n          </div>\n        </div>\n\n        <hr class=\"horizontal dark my-4\" />\n        <div class=\"mt-2 mb-5 d-flex\">\n          <h6 class=\"mb-0\">Light / Dark</h6>\n          <div class=\"form-check form-switch ps-0 ms-auto my-auto\">\n            <input\n              class=\"form-check-input mt-1 ms-auto\"\n              type=\"checkbox\"\n              :checked=\"store.state.darkMode\"\n              @click=\"darkMode\"\n            />\n          </div>\n        </div>\n        <a\n          class=\"btn bg-gradient-dark w-100\"\n          href=\"https://www.creative-tim.com/product/vue-argon-dashboard\"\n          >Free Download</a\n        >\n        <a\n          class=\"btn btn-outline-dark w-100\"\n          href=\"https://www.creative-tim.com/learning-lab/vue/overview/argon-dashboard/\"\n          >View documentation</a\n        >\n        <div class=\"text-center w-100\">\n          <a\n            class=\"github-button\"\n            href=\"https://github.com/creativetimofficial/vue-argon-dashboard\"\n            data-icon=\"octicon-star\"\n            data-size=\"large\"\n            data-show-count=\"true\"\n            aria-label=\"Star creativetimofficial/vue-argon-dashboard on GitHub\"\n            >Star</a\n          >\n          <h6 class=\"mt-3\">Thank you for sharing!</h6>\n          <a\n            href=\"https://twitter.com/intent/tweet?text=Check%20Vue%20Argon%20Dashboard%202%20made%20by%20%40CreativeTim%20%23webdesign%20%23dashboard%20%vuejs3&amp;url=https%3A%2F%2Fwww.creative-tim.com%2Fproduct%vue-argon-dashboard\"\n            class=\"mb-0 btn btn-dark me-2\"\n            target=\"_blank\"\n          >\n            <i class=\"fab fa-twitter me-1\" aria-hidden=\"true\"></i> Tweet\n          </a>\n          <a\n            href=\"https://www.facebook.com/sharer/sharer.php?u=https://www.creative-tim.com/product/vue-argon-dashboard\"\n            class=\"mb-0 btn btn-dark me-2\"\n            target=\"_blank\"\n          >\n            <i class=\"fab fa-facebook-square me-1\" aria-hidden=\"true\"></i> Share\n          </a>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n"],"mappings":"AACA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,gBAAgB,EAAEC,kBAAkB,QAAQ,uBAAuB;;;;;;;IAE5E,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB;IACA,MAAMI,UAAU,GAAGL,QAAQ,CAAC,MAAMI,KAAK,CAACE,KAAK,CAACD,UAAU,CAAC;IACzD,MAAME,WAAW,GAAGP,QAAQ,CAAC,MAAMI,KAAK,CAACE,KAAK,CAACC,WAAW,CAAC;IAC3D,MAAMC,kBAAkB,GAAGA,CAAA,KAAMJ,KAAK,CAACK,MAAM,CAAC,oBAAoB,CAAC;;IAEnE;IACA,MAAMC,WAAW,GAAGA,CAAA,KAAMN,KAAK,CAACK,MAAM,CAAC,aAAa,CAAC;IACrD,MAAME,cAAc,GAAIC,IAAI,IAAKR,KAAK,CAACK,MAAM,CAAC,aAAa,EAAEG,IAAI,CAAC;IAElE,MAAMC,YAAY,GAAGA,CAACC,KAAK,GAAG,SAAS,KAAK;MAC1CC,QAAQ,CAACC,aAAa,CAAC,eAAe,CAAC,CAACC,YAAY,CAAC,YAAY,EAAEH,KAAK,CAAC;IAC3E,CAAC;IAED,MAAMI,QAAQ,GAAGA,CAAA,KAAM;MACrB,IAAId,KAAK,CAACE,KAAK,CAACY,QAAQ,EAAE;QACxBd,KAAK,CAACE,KAAK,CAACY,QAAQ,GAAG,KAAK;QAC5BP,cAAc,CAAC,UAAU,CAAC;QAC1BR,kBAAkB,CAAC,CAAC;QACpB;MACF,CAAC,MAAM;QACLC,KAAK,CAACE,KAAK,CAACY,QAAQ,GAAG,IAAI;QAC3BP,c